<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SetBomArticleDetailsView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="ncl.pm.model.bc.vo.misc.SetBomArticleDetailsView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT C.COMB_ID,
                C.SEGMENT2,
                C.DESCRIPTION,
                M.PRIMARY_UNIT_OF_MEASURE UOM,
                A.QUANTITY
  FROM XXPM_MADEUP_COMBINATIONS C,
       MTL_SYSTEM_ITEMS M,
       XXPM_MADEUP_SET_ARTICLES A
 WHERE     C.COMB_ID = M.ATTRIBUTE26
       AND M.ORGANIZATION_ID = 87
       AND M.SEGMENT1 = '24'
       AND M.SEGMENT3 = '31'
       AND C.ARTICLE_ID = A.ARTICLE_ID]]>
  </SQLQuery>
  <ViewAttribute
    Name="CombId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="COMB_ID"
    Expression="COMB_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Segment2"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SEGMENT2"
    Expression="SEGMENT2"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Description"
    IsUpdateable="false"
    IsPersistent="false"
