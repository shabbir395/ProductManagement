  <Variable
    Name="BindSoHeaderId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery>
    <![CDATA[SELECT   PHA.SEGMENT1 PO_NUM,
         PV.VENDOR_NAME,
         XPL.PO_LINE_ID,
         --       xph.prog_id,
         XPH.SO_HEADER_ID,
         --       pla.item_id,
         MSI.CONCATENATED_SEGMENTS ITEM_CODE,
         PLA.ITEM_DESCRIPTION,
         PLA.UNIT_MEAS_LOOKUP_CODE UOM,
         PLA.UNIT_PRICE,
         XPL.REMARKS,
         PLA.QUANTITY ORDERED_QUANTITY,
         NVL (SUM (PLLA.QUANTITY_RECEIVED), 0) QUANTITY_RECEIVED,
         NVL(PLA.QUANTITY, 0) - NVL (SUM (PLLA.QUANTITY_RECEIVED), 0) QUANTITY_BALANCE,
         NVL (SUM (PLLA.QUANTITY_ACCEPTED), 0) ACCEPTED_QUANTITY,
         NVL (SUM (PLLA.QUANTITY_REJECTED), 0) QUANTITY_REJECTED
    FROM PO_LINE_LOCATIONS_ALL PLLA,
         PO_LINES_ALL PLA,
         PO_HEADERS_ALL PHA,
         PO_VENDORS PV,
         XXPM_PO_LINES XPL,
         XXPM_PO_HEADER XPH,
         MTL_SYSTEM_ITEMS_KFV MSI
   WHERE PLLA.PO_LINE_ID = PLA.PO_LINE_ID
     AND PLA.ATTRIBUTE15 = XPL.PO_LINE_ID
     AND PHA.PO_HEADER_ID = PLA.PO_HEADER_ID
     AND PV.VENDOR_ID = PHA.VENDOR_ID
     AND XPL.PO_HEADER_ID = XPH.PO_HEADER_ID
     AND PLA.ITEM_ID = MSI.INVENTORY_ITEM_ID
     AND MSI.ORGANIZATION_ID = 106 --stitching 1 periodic
     AND XPL.ITEM_TYPE = 1
     AND XPH.SO_HEADER_ID = :BindSoHeaderId
GROUP BY PHA.SEGMENT1,
         PV.VENDOR_NAME,
         XPL.PO_LINE_ID,
         --       xph.prog_id,
         XPH.SO_HEADER_ID,
         --       pla.item_id,
         MSI.CONCATENATED_SEGMENTS,
         PLA.ITEM_DESCRIPTION,
         PLA.UNIT_MEAS_LOOKUP_CODE,
         PLA.UNIT_PRICE,
         XPL.REMARKS,
         PLA.QUANTITY]]>
  </SQLQuery>
