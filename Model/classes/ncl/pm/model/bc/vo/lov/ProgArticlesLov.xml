<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ProgArticlesLov"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="ncl.pm.model.bc.vo.lov.ProgArticlesLov_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindProgId"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <SQLQuery>
    <![CDATA[SELECT A.ARTICLE_ID, A.ARTICLE_NAME
  FROM XXPM_MADEUP_ARTICLES A
 WHERE A.PROG_ID = :BindProgId
   AND NVL (A.SET_ARTICLE, 'A') = 'Set']]>
  </SQLQuery>
  <ViewAttribute
    Name="ArticleId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ARTICLE_ID"
    Expression="ARTICLE_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ArticleName"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ARTICLE_NAME"
    Expression="ARTICLE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ArticleId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="ncl.pm.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
