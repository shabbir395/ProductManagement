  <SQLQuery>
    <![CDATA[SELECT   X.IM_ID,
         X.SEGMENT1,
         X.SEGMENT2,
         X.DESCRIPTION,
         X.ACC_ACCESSORY_TYPE,
         X.SEGMENT7,
         X.ACC_WASTAGE,
         P.PROG_ID,
         X.SEGMENT4 COLOR_CODE,
         XXPM_GET_COLOR_DESC (X.SEGMENT4) ITEM_COLOR,
         X.SEGMENT5 SIZE_CODE,
         XXPM_GET_SIZE_DESC (X.SEGMENT5) ITEM_SIZE,
         MAX (PRIMARY_UNIT_OF_MEASURE) UOM
    FROM XXPM_ITEM_MASTER X, MTL_SYSTEM_ITEMS M, XXPM_MADEUP_PROGRAMS P
   WHERE X.SEGMENT7 = P.PROG_CODE
     AND X.IM_ID = M.ATTRIBUTE26
     AND X.SEGMENT1 IN ('14')
     AND M.SEGMENT1 IN ('14')
     AND M.ORGANIZATION_ID = 106
     AND X.ITEM_STATUS_FLAG = 1
GROUP BY X.IM_ID,
         X.SEGMENT1,
         X.SEGMENT2,
         X.DESCRIPTION,
         X.ACC_ACCESSORY_TYPE,
         P.PROG_ID,
         X.SEGMENT7,
         X.ACC_WASTAGE,
         X.SEGMENT4,
         X.SEGMENT5]]>
